<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.polytech.tictactoe.view.JavaFXTicTacToeViewController">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <GridPane fx:id="gridPane" alignment="CENTER" gridLinesVisible="true" VBox.vgrow="ALWAYS">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="128.0" prefWidth="128.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="128.0" prefWidth="128.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="128.0" prefWidth="128.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="128.0" prefHeight="128.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="128.0" prefHeight="128.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="128.0" prefHeight="128.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </children>
      </VBox>
      <VBox alignment="CENTER" spacing="20.0">
         <children>
            <Label fx:id="gameResult" VBox.vgrow="ALWAYS" />
            <Label fx:id="playerOne" VBox.vgrow="ALWAYS" />
            <Label fx:id="playerTwo" VBox.vgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
